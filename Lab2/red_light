#include "pico/stdlib.h" //Include the standard library


int main() {
	gpio_init(22); //Initiate GPIO pin 22
	gpio_set_dir(22, GPIO_OUT); //Set pin 22 as an output
	
	
		
	while(true) { //Create an infinite loop
		for (int i=0; i<5;i++){ //Create for loop to repeat five times at 1Hz
			gpio_put(22,1); //Write a 1 to GPIO pin 22.
			sleep_ms(500); //Wait 500ms
			gpio_put(22,0); //Write a 0 to GPIO pin 22.
			sleep_ms(500); //Wait 500ms
		}
		
		for (int i = 0; i<5;i++){ //Create for loop to repeat five times at 5Hz
			gpio_put(22,1); //Write a 1 to GPIO pin 22.
			sleep_ms(200); //Wait 200ms
			gpio_put(22,0); //Write a 0 to GPIO pin 22.
			sleep_ms(200); //Wait 200ms
		}
		}
	return(0);
}
