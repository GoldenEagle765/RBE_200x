#include "pico/stdlib.h"


int main() {
	gpio_init(22);
	gpio_set_dir(22, GPIO_OUT);
	
	
		
	while(true) {
		for (int i=0; i<5;i++){
			gpio_put(22,1);
			sleep_ms(500);
			gpio_put(22,0);
			sleep_ms(500);
		}
		
		for (int i = 0; i<5;i++){
			gpio_put(22,1);
			sleep_ms(200);
			gpio_put(22,0);
			sleep_ms(200);
		}
		}
	return(0);
}
